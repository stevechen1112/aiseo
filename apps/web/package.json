{
  "name": "@aiseo/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build && node scripts/prepare-standalone.mjs",
    "start": "node .next/standalone/apps/web/server.js",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:install": "playwright install --with-deps",
    "perf:lighthouse": "lighthouse http://127.0.0.1:3000/dashboard --quiet --no-enable-error-reporting --chrome-flags=\"--headless\" --output=json --output-path=./lighthouse-report.json",
    "perf:lighthouse:prod": "lighthouse http://127.0.0.1:3000/dashboard --quiet --no-enable-error-reporting --chrome-flags=\"--headless\" --output=json --output-path=./lighthouse-report.prod.json"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@rive-app/react-canvas": "^4.27.0",
    "@tanstack/react-query": "^5.17.0",
    "@tanstack/react-virtual": "^3.13.18",
    "@tiptap/extension-character-count": "^3.19.0",
    "@tiptap/extension-placeholder": "^3.19.0",
    "@tiptap/pm": "^3.19.0",
    "@tiptap/react": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cytoscape": "^3.33.1",
    "date-fns": "^3.3.0",
    "framer-motion": "^12.34.2",
    "lucide-react": "^0.344.0",
    "next": "^15.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "recharts": "^2.12.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.0.0",
    "lighthouse": "^12.4.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.0"
  }
}
